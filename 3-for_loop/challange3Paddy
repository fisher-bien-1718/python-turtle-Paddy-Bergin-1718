from turtle import*
#this will make a grid from squares
for t in range(0, 4):
 forward(90)
 right(90)
 forward(90)
 right(90)
 forward(90)
 right(90)
 forward(90)
up()
goto(-90,45)
down()
for z in range(0,1):
    forward(180)
    right(90)
    forward(90)
    right(90)
    forward(180)
up()
goto(45,90)
down()
for p in range(0,1):
    left(90)
    forward(180)
    right(90)
    forward(90)
    right(90)
    forward(180)
    


